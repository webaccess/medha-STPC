{
  "connection": "default",
  "collectionName": "individuals",
  "info": {
    "name": "Individual"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "first_name": {
      "type": "string",
      "required": false
    },
    "last_name": {
      "type": "string"
    },
    "contact": {
      "plugin": "crm-plugin",
      "model": "contact",
      "via": "individual"
    },
    "stream": {
      "model": "stream"
    },
    "is_verified": {
      "type": "boolean",
      "default": false
    },
    "is_physically_challenged": {
      "type": "boolean",
      "default": false
    },
    "date_of_birth": {
      "type": "datetime"
    },
    "gender": {
      "type": "enumeration",
      "enum": ["male", "female", "other"],
      "required": false
    },
    "roll_number": {
      "type": "integer",
      "required": false
    },
    "organization": {
      "plugin": "crm-plugin",
      "model": "organization"
    },
    "profile_photo": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "father_full_name": {
      "type": "string"
    },
    "mother_full_name": {
      "type": "string"
    },
    "middle_name": {
      "type": "string"
    },
    "future_aspirations": {
      "collection": "futureaspirations"
    }
  }
}
