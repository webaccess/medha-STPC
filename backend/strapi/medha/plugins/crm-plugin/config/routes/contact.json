{
  "routes": [
    {
      "method": "GET",
      "path": "/contact",
      "handler": "contact.find",
      "config": {
        "policies": ["plugins::users-permissions.isAuthenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/contact/:id",
      "handler": "contact.findOne",
      "config": {
        "policies": ["plugins::users-permissions.isAuthenticated"]
      }
    },
    {
      "method": "POST",
      "path": "/contact",
      "handler": "contact.create",
      "config": {
        "policies": ["plugins::users-permissions.isAuthenticated"]
      }
    },
    {
      "method": "PUT",
      "path": "/contact/:id",
      "handler": "contact.update",
      "config": {
        "policies": ["plugins::users-permissions.isAuthenticated"]
      }
    },
    {
      "method": "DELETE",
      "path": "/contact/:id",
      "handler": "contact.delete",
      "config": {
        "policies": ["plugins::users-permissions.isAuthenticated"]
      }
    }
  ]
}
