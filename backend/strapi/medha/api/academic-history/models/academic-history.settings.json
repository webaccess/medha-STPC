{
  "collectionName": "academic_histories",
  "info": {
    "name": "Academic History"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "contact": {
      "plugin": "crm-plugin",
      "model": "contact"
    },
    "academic_year": {
      "model": "academic-year"
    },
    "percentage": {
      "type": "float"
    },
    "education_year": {
      "type": "enumeration",
      "enum": [
        "First",
        "Second",
        "Third",
        "Fourth"
      ]
    }
  },
  "connection": "default"
}
