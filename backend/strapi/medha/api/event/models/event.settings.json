{
  "connection": "default",
  "collectionName": "events",
  "info": {
    "name": "Event"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "start_date_time": {
      "type": "datetime",
      "required": true
    },
    "end_date_time": {
      "type": "datetime",
      "required": true
    },
    "rpc": {
      "model": "rpc"
    },
    "colleges": {
      "collection": "college"
    },
    "streams": {
      "collection": "stream"
    },
    "age": {
      "type": "integer",
      "required": true
    },
    "upload_logo": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "question_set": {
      "model": "question-set",
      "via": "events"
    },
    "address": {
      "type": "text",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    }
  }
}
