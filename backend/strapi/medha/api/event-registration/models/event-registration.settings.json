{
  "kind": "collectionType",
  "collectionName": "event_registrations",
  "info": {
    "name": "Event Registration"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "event": {
      "model": "event"
    },
    "contact": {
      "plugin": "crm-plugin",
      "model": "contact"
    },
    "is_attendance_verified": {
      "type": "boolean",
      "default": false
    },
    "is_hired_at_event": {
      "type": "boolean",
      "default": false
    },
    "is_plan_on_attending": {
      "type": "boolean",
      "default": false
    }
  }
}
