{
  "connection": "default",
  "collectionName": "educations",
  "info": {
    "name": "Education"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "year_of_passing": {
      "model": "academic-year"
    },
    "education_year": {
      "type": "enumeration",
      "enum": [
        "first",
        "second",
        "third"
      ],
      "required": false
    },
    "marks": {
      "type": "decimal"
    },
    "qualification": {
      "type": "enumeration",
      "enum": [
        "secondary",
        "senior_secondary",
        "undergraduate",
        "postgraduate",
        "other"
      ]
    },
    "institute": {
      "type": "string"
    },
    "pursuing": {
      "type": "boolean",
      "default": false
    },
    "contact": {
      "plugin": "crm-plugin",
      "model": "contact"
    },
    "board": {
      "model": "board"
    },
    "other_qualification": {
      "type": "string"
    }
  }
}
