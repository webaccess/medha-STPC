{
  "routes": [
    {
      "method": "GET",
      "path": "/educations",
      "handler": "education.find",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/educations/count",
      "handler": "education.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/educations/:id",
      "handler": "education.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/educations",
      "handler": "education.create",
      "config": {
        "policies": ["isOwner"]
      }
    },
    {
      "method": "PUT",
      "path": "/educations/:id",
      "handler": "education.update",
      "config": {
        "policies": ["isOwner"]
      }
    },
    {
      "method": "DELETE",
      "path": "/educations/:id",
      "handler": "education.delete",
      "config": {
        "policies": ["isOwner"]
      }
    }
  ]
}
