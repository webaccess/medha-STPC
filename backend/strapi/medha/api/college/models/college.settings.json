{
  "connection": "default",
  "collectionName": "colleges",
  "info": {
    "name": "College"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "rpc": {
      "model": "rpc"
    },
    "college_code": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "address": {
      "type": "richtext",
      "required": true
    },
    "contact_number": {
      "type": "string"
    },
    "college_email": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "streams": {
      "collection": "stream"
    },
    "principal": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
