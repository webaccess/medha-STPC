{
  "mobileNumber": {
    "label": "Mobile Number",
    "id": "mobileNumber",
    "autoComplete": "mobileNumber",
    "required": true,
    "type": "text",
    "placeholder": "Your 10 digit registered mobile number",
    "autoFocus": true,
    "validations": {
      "required": {
        "value": "true",
        "message": "Mobile number is required"
      },
      "validateMobileNumber": {
        "value": 10,
        "message": "Please enter a valid mobile number"
      }
    }
  },
  "newPassword": {
    "label": "New Password",
    "id": "newPassword",
    "type": "password",
    "required": true,
    "validations": {
      "required": {
        "value": "true",
        "message": "password is required"
      },
      "validatePasswordRegex": {
        "value": "true",
        "message": "Password must contain numbers and letters and should be atleast 8 characters long"
      }
    }
  },
  "confirmNewPassword": {
    "label": "Confirm New Password",
    "id": "confirmNewPassword",
    "type": "password",
    "required": true,
    "validations": {
      "required": {
        "value": "true",
        "message": "confirm password is required"
      }
    }
  },
  "OTP": {
    "label": "OTP",
    "id": "OTP",
    "type": "password",
    "required": true,
    "validations": {
      "required": {
        "value": "true",
        "message": "OTP is required"
      },
      "validateOtpForForgotPassword": {
        "value": 4,
        "message": "Please enter a valid OTP"
      }
    }
  }
